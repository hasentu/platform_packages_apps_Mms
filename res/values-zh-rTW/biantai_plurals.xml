<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Template delete confirmation -->
    <!-- 模板删除确认 -->
    <plurals name="template_confirm_delete_conversation">
        <item quantity="one"><xliff:g id="number">%1$s</xliff:g>個模板將被刪除</item>
        <item quantity="other"><xliff:g id="number" example="15">%1$s</xliff:g>個模板將被刪除</item>
    </plurals>

    <!-- Remaining delay time -->
    <!-- 剩余的取消发送延时时间 -->
    <plurals name="remaining_delay_time">
        <item quantity="one">%1$d秒後送出, 觸摸以取消</item>
        <item quantity="other">%1$d秒後送出, 觸摸以取消</item>
    </plurals>

    <!-- Mark as unread confirmation -->
    <!-- 标记为未读确认 -->
    <plurals name="confirm_mark_unread_conversation">
        <item quantity="one"><xliff:g id="number">%1$s</xliff:g>個對話將被標記爲未讀</item>
        <item quantity="other"><xliff:g id="number" example="15">%1$s</xliff:g>個對話將被標記爲未讀</item>
    </plurals>

</resources>
